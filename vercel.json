{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/config",
        "dest": "/api/config"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
}
  